<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neusoft.bsp.business.bvo.mapper.WishlistMapper">


    <select id="getById" parameterType="java.lang.Integer" resultType="Wishlist">
     select * from wit_wishlist where wit_id = #{wit_id}
  </select>

    <select id="getAllById" parameterType="java.lang.Integer" resultType="Wishlist">
        select * from wit_wishlist where dsr_id = #{dsr_id}
    </select>

    <delete id="delete" parameterType="java.lang.Integer">
    delete from wit_wishlist
    where wit_id = #{wit_id}
  </delete>

    <insert id="insert" parameterType="Brand" useGeneratedKeys="true" keyProperty="wit_id">
        insert into wit_wishlist values(null,#{dsr_id},#{pro_id},#{created_by},
        #{creation_date},#{last_update_by},#{last_update_date},#{call_cnt},#{remark},#{sts_cd})
    </insert>

    <update id="update">
        update wit_wishlist set dsr_id = #{dsr_id},pro_id = #{pro_id},
        created_by = #{created_by}, creation_date = #{creation_date},last_update_by = #{last_update_by},
        last_update_date= #{last_update_date},call_cnt =#{call_cnt},remark =#{remark},sts_cd=#{sts_cd}
        where wit_id = #{wit_id}
    </update>





</mapper>